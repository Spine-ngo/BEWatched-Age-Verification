{
  "compilerOptions": {
    "module": "ESnext",
    "target": "ESnext",
    "sourceMap": true,
    "jsxFactory": "h",
    "jsxFragmentFactory": "DocumentFragment",
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
