{
  "name": "bewatched_age_verification",
  "author": "Bram Verdyck",
  "version": "0.0.1",
  "description": "Optimize resources for the Bewatched Age Verification plugin",
  "README": "./taskmanager/README.md",
  "private": true,
  "type": "module",
  "scripts": {
    "prestart": "npm install",
    "start": "npm run dev",
    "dev": "node --experimental-json-modules -r dotenv/config node_modules/gulp/bin/gulp watch --gulpfile ./taskmanager/gulpfile.js",
    "build": "node --experimental-json-modules -r dotenv/config node_modules/gulp/bin/gulp build --gulpfile ./taskmanager/gulpfile.js"
  },
  "license": "ISC",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": 2,
      "no-undef": 2
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.13",
    "cssnano": "^4.1.10",
    "dart-sass": "^1.25.0",
    "eslint": "^7.12.1",
    "eslint-webpack-plugin": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-webp": "^4.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.0.0",
    "webpack-dev-middleware": "^4.0.0",
    "webpack-stream": "^6.1.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "gulp-rename": "^2.0.0"
  }
}
